<div class="container">
<h1>Listing Diet Ingredient Types</h1>

<table class="table">
  <thead>
    <tr>
      <th>Diet</th>
      <th>Ingredient type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diet_ingredient_types.each do |diet_ingredient_type| %>
      <tr>
        <td><%= diet_ingredient_type.diet.name %></td>
        <td><%= diet_ingredient_type.ingredient_type.name %></td>
        <%= render 'view_diet_ingredient_type', diet_ingredient_type: diet_ingredient_type %>
        <% if current_user.admin_user? then %>
          <%= render 'change_diet_ingredient_type', diet_ingredient_type: diet_ingredient_type %>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if current_user.admin_user? then %>
  <%= link_to 'New Diet ingredient type', new_diet_ingredient_type_path %>
<% end %>
</div>
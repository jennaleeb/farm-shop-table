<div class="container">
<p>
  <strong>Name:</strong>
  <%= @diet.name %>
</p>

<p>
	<strong>Ingredient Types:</strong>
	<ul>
	<% @diet.ingredient_types.each do |ingredient_type| %>
		<li><%= ingredient_type.name %></li>
	<% end %>
	</ul>
</p>

<%= link_to 'Change', edit_diet_path(@diet) %> |
<%= link_to 'Back to Diet Definitions', diets_path %>
</div>